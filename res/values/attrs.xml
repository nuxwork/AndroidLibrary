<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SeekBar">
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="progressHeight" format="dimension" />
        <attr name="progressDrawable" format="reference|color" />
        <attr name="thumb" format="reference|color" />
        <attr name="thumbWidth" format="dimension" />
        <attr name="thumbHeight" format="dimension" />
        <attr name="thumbOffset" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CircleLayout">
        <attr name="rotationOffset" format="float" />
        <attr name="startAngle" format="float" />
    </declare-styleable>
    <declare-styleable name="NumberView">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="reference|color" />
        <attr name="angle" format="dimension" />
    </declare-styleable>
    <declare-styleable name="Switcher">
        <attr name="checked" format="boolean" />
        <attr name="thumb" />
        <attr name="textOn" format="string" />
        <attr name="textOff" format="string" />
        <attr name="textSize" />
        <attr name="textColor" />
        <attr name="textAppearance" format="reference"/>
    </declare-styleable>

</resources>